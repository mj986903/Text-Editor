<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Text_Converter</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>Converter</display-name>
    <servlet-name>Converter</servlet-name>
    <servlet-class>com.mohit.converter.Converter</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Converter</servlet-name>
    <url-pattern>/Converter</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CopyClipboard</display-name>
    <servlet-name>CopyClipboard</servlet-name>
    <servlet-class>com.mohit.converter.CopyClipboard</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CopyClipboard</servlet-name>
    <url-pattern>/CopyClipboard</url-pattern>
  </servlet-mapping>
  <jsp-config>
  <jsp-property-group>
    <url-pattern>*.jsp</url-pattern>
    <trim-directive-whitespaces>true</trim-directive-whitespaces>
  </jsp-property-group>
</jsp-config>
</web-app>